# skript 2.13.2 = true
# addons = none
# the above information is comments


command /givedirt:
    trigger:
        inv(player)

function inv(p: player):
    # creates the inventory and assigns it to variable
    set {_inv} to chest inventory with 1 row 
    set slot integers between 0 and 8 of {_inv} to dirt

    open {_inv} to {_p}
